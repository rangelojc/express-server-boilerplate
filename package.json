{
    "name": "express-server-boilerplate",
    "version": "0.0.0",
    "private": false,
    "scripts": {
        "start": "node ./src/bin/www",
        "server": "babel-node ./src/bin/www",
        "dev": "NODE_ENV=development npm-run-all server",
        "prod": "NODE_ENV=production npm-run-all server",
        "watch": "nodemon --exec NODE_ENV=development npm-run-all server"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.18.2",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1",
        "npm-run-all": "^4.1.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.0",
        "@babel/core": "^7.7.2",
        "@babel/node": "^7.7.0",
        "@babel/preset-env": "^7.7.1",
        "nodemon": "^1.19.4"
    }
}